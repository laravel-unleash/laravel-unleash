{
    "name": "mikefrancis/laravel-unleash",
    "description": "An Unleash client for Laravel",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "illuminate/support": "^5.8|^6|^7|^8|^9|^10.0",
        "illuminate/http": "^5.8|^6|^7|^8|^9|^10.0",
        "illuminate/contracts": "^5.8|^6|^7|^8|^9|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "LaravelUnleash\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelUnleash\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Francis",
            "email": "mikeffrancis@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "LaravelUnleash\\ServiceProvider"
            ],
            "aliases": {
                "Unleash": "LaravelUnleash\\Facades\\Unleash",
                "Feature": "LaravelUnleash\\Facades\\Feature"
            }
        }
    }
}
